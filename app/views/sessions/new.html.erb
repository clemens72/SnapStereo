<% provide(:title, "Sign In") %>
<h1>Sign In</h1>
 <% if flash.now[:error] %>
    <%= flash.now[:error] %>
  <% end %>
    <%= form_for(:session, url: sessions_path) do |f| %>

      <%= f.label :email %>
      <%= f.text_field :email %>

      <%= f.label :password %>
      <%= f.password_field :password %>

      <%= f.submit "Sign in"%>
    <% end %>
